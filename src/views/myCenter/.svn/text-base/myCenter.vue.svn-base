<script type="text/ecmascript-6">
    import router from '../../router';
    import footer from '../../components/footer/Footer.vue';
    import cShare from './zhoufanShare/zhoufanShare.vue';
    export default{
        name:'myCenter',
        data () {
            return{
                headPicUrl:'data:image/png;base64,',
                userName:'注册/登录',
                myCenterData:'',
                loginStates:false,
                bottomShow:false,
                messageNew:false

            }
        },
        created () {

        },
        mounted () {
            this.getName ();
//            let self = this;
            window.scrollTo(0,0);
//            this.$nextTick(function(){
//                 self.bottomShow = true
//
//            });

//            this.bindWindow()
        },
        methods: {
            getName () {
                let self = this;
//                if(self.find('memberData')){
//                    self.myCenterData = self.find('memberData');
//
//                }
                self.logGetQuery(true,CTX_URL + '/index/member/info',response => {
                    if(response.data.messageCount != 0 ){
                        self.messageNew = true
                    }
                    self.myCenterData = response.data;

//                    self.$store.commit('loginIn');
                    self.loginStates = true;
                    self.headPicUrl = IMG_URL + response.data.memberDTO.gravatar;

                    if(response.data.memberDTO.nickname == "" || response.data.memberDTO.nickname == undefined){
                        self.userName = response.data.memberDTO.usercode.substring(0,3)
                                            + "****"
                                            + response.data.memberDTO.usercode.substring(7,11);

                    } else {
                        self.userName = response.data.memberDTO.nickname;
                    }

                    self.save('memberData',self.myCenterData)
                })
            },
            pushToSetting () {
                let self = this;
                let url = "/myCenter/settingsPage";

                this.islogin (function(){
                    self.$root.pushView(url)
                },url)

            },
            showAddress () {
                let self = this;
                let url = "/myCenter/addressList";

                this.islogin (function(){
                    self.$root.pushView(url)
                },url)
            },
            showCollection () {
                let self = this;
                let url = "/myCenter/collection";

                this.islogin (function(){
                    self.$root.pushView(url)
                },url)
            },
            showBuyOrder (index) {
                let self = this;
                let url = "/myCenter/buyOrderList";

                this.islogin(function(){
//                    console.log(index)
                    if(index === undefined ) {
                        self.pushView(url,{
                            tab:0
                        })
                    }
                    else {
                        self.pushView(url,{
                            tab:index
                        })
                    }

                },url)
            },
            showExpOrder (index) {
                let self = this;
                let url = "/myCenter/experienceOrderList";

                this.islogin(function(){
                    if(index === undefined ) {
                        self.pushView(url,{
                            tab:0
                        })
                    }
                    else{
                        self.pushView(url,{
                            tab:index
                        })
                    }

                },url)
            },

            islogin (callback,targetUrl) {
                let self = this;

                if(self.loginStates === true) {
                    callback()
                }else {
                    self.$root.confirm("您还没有登录，现在登录？",function (type) {
                        if(type === "1") {
                            self.pushView("/signIn")
                        }
                    })
                }
            },
            loginMethod () {
                if (this.loginStates === false) {
                    this.pushView("/signIn")
                } else {
                    this.pushView("/myCenter/settingsPage")
                }
            },
            showMyAccount (){
                let self = this;
                let url = "/myCenter/myAccount";

                this.islogin (function(){
                    self.$root.pushView(url)
                },url)
            },
            showMyIntegral () {
                let self = this;
                let url = "/myCenter/myIntegra";

                this.islogin (function(){
                    self.$root.pushView(url)
                },url)
            },
            showCoupon () {
                let self = this;
                let url = "/coupon";

                this.islogin (function(){
                    self.$root.pushView(url)
                },url)
            },
            goodsArriveRemind () {
                let self = this;
                let url = "/myCenter/goodsArriveRemind";

                this.islogin (function(){
                    self.pushView(url)
                },url)
            },
            showMessage: function(){
                this.pushView('/myNews');
            },
            showHelp () {
                this.pushView('/myCenter/helpCenter');
            },
            showShare () {
                this.$refs.shareP.showPage()
//                console.log( this.$refs.shareP)
            },
            bindWindow () {
                document.getElementsByTagName('html')[0].style.height = "auto";
                document.getElementsByTagName('body')[0].style.height = "auto";
                this.$root.$el.style.height = "auto";

//                this.bottomShow = true
            },
        },
        components:{
            'v-footer':footer,
            cShare
        },
        beforeDestroy () {
            document.getElementsByTagName('html')[0].style.height = "100%";
            document.getElementsByTagName('body')[0].style.height = "100%";
            this.$root.$el.style.height = "100%";

        },
    }
</script>
